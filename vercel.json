{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "landing/**", "use": "@vercel/static" },
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/landing$", "dest": "/landing/index.html" },
    { "src": "^/suppliers.html$", "dest": "/public/suppliers.html" }
  ]
}
